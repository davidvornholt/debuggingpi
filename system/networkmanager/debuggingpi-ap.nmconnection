[connection]
id=DebuggingPi-AP
type=wifi
interface-name=wlan0
autoconnect=true
autoconnect-priority=100

[wifi]
mode=ap
ssid=DebuggingPi
band=bg
channel=6

[wifi-security]
key-mgmt=wpa-psk
psk=debuggingpi
proto=rsn
pairwise=ccmp
group=ccmp

[ipv4]
method=shared
address1=10.42.0.1/24

[ipv6]
method=disabled
